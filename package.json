{
  "name": "stocktracker",
  "author": "Joe McIntyre (j_h_mcintyre@yahoo.com)",
  "description": "Stock Tracker",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fcc-joemcintyre/stocktracker"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.15.1",
    "express": "4.14.0",
    "request": "2.72.0",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "socket.io": "1.3.7",
    "d3": "3.5.17",
    "react-faux-dom": "2.5.0"
  },
  "scripts": {
    "test": "npm run test-client && npm run test-server",
    "test-client": "mocha test/client/**/*.js --compilers js:babel-core/register",
    "test-server": "mocha test/server/test-main.js",
    "start": "nodemon dist/main.js --port=3000",
    "build": "gulp",
    "build-stage": "gulp stage",
    "coverage": "npm run c-server && npm run c-combine",
    "c-server": "./node_modules/istanbul/lib/cli.js cover --dir ./coverage/server ./node_modules/mocha/bin/_mocha test/server/test-main.js -- -R spec",
    "c-combine": "./node_modules/istanbul/lib/cli.js report --dir ./coverage --include **/*coverage.json json lcov"
  },
  "devDependencies": {
    "babel-core": "6.9.1",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babelify": "7.3.0",
    "browserify": "13.0.1",
    "expect": "1.20.1",
    "expect-jsx": "2.6.0",
    "gulp": "3.9.1",
    "gulp-cssmin": "0.1.7",
    "gulp-gzip": "1.4.0",
    "gulp-sass": "2.3.2",
    "gulp-sourcemaps": "1.6.0",
    "gulp-uglify": "1.5.3",
    "gulp-util": "3.0.7",
    "istanbul": "0.4.3",
    "mocha": "2.5.3",
    "nodemon": "1.9.2",
    "react-addons-test-utils": "15.1.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.7.0"
  },
  "engines": {
    "node": "6.2.1"
  }
}
